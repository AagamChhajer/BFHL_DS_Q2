{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ],
    "env": {
        "PYTHONPATH": ".",
        "PYTESSERACT_PATH": "/usr/bin/tesseract"
    }
}